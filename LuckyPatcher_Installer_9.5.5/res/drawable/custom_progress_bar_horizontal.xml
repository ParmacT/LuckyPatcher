<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ff000000" android:endColor="#ff000000" android:angle="270.0" android:centerY="0.5" android:centerColor="#ffbfab54" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff000000" android:endColor="#ff000000" android:angle="270.0" android:centerY="0.5" android:centerColor="#771997e1" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff000000" android:endColor="#ff000000" android:angle="270.0" android:centerY="0.5" android:centerColor="#ff1997e1" />
            </shape>
        </clip>
    </item>
</layer-list>